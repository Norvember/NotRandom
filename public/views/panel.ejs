<div class="col" id="<%= pageId %>" style="display: <%= shouldDisplay %>;">
    <h2>Priority</h2>
    <p>This is the content for the Priority page.</p>
    <div class="text-center">
      <input
        type="text"
        class="form-control"
        placeholder="Enter text"
        style="margin-bottom: 10px"
      />
      <div class="input-group">
        <input
          type="number"
          class="form-control"
          placeholder="Min"
          style="margin-right: 5px"
        />
        <input
          type="number"
          class="form-control"
          placeholder="Max"
          style="margin-left: 5px"
        />
      </div>
      <button class="btn btn-primary" style="margin-top: 10px">
        Generate
      </button>
    </div>
  </div>